{
  "name": "colorguide",
  "main": "./lib/colorguide",
  "version": "0.0.0",
  "description": "A colorguide to the current UI & Syntax colors",
  "keywords": [
    "theme colors",
    "colors",
    "syntax colors",
    "ui colors"
  ],
  "activationCommands": {
    "atom-workspace": "colorguide:show"
  },
  "repository": "https://github.com/morbius1st/colorguide",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0",
    "underscore-plus": "^1.0.0",
    "jquery": "^2.1.4",
    "fs-plus": "^2.8.2",
    "path": "^0.12.7",
    "mathjs": "^1.5.0",
    "tool-bar": "^1.0.0"
  },
  "consumedServices": {
    "themeVariables": {
      "versions": {
        ">=0.0.0": "consumeThemeVars"
      }
    }
  }
}
